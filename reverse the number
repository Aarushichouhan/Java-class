import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	Scanner sc=new Scanner(System.in); 
	System.out.println("Enter a number:");
	int a=sc.nextInt();
	int rev=0,q;
	while(a!=0)
	{
	    q=a%10;
	    rev=(rev*10)+q;
	    a=a/10;
	}
	System.out.println("Reverse of number is:"+ rev);
	
	}
}
