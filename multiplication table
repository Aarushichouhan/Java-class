import java.util.Scanner;
 public class Multiplication{
     public static void main(String[] args){
      Scanner sc=newScanner(System.in);
       System.out.println("Enter a number");
         int n=sc.nextInt();
         for(int i=1;i<=10;i++)
         {
             System.out.println(n+"x"+i+"="+n*i);
         }
         
     }
    
 }
